{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "semi": ["warn", "always"],
        "quotes": ["warn", "single"],
        "comma-dangle":["warn","only-multiline"],
        "no-underscore-dangle":["warn","never"]
    }
}
